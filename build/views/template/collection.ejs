<div class="section-v-list">
    <ul class="v-list">
        <% for(var i = 0; i < collectionList.length; i++ ) { %>
            <li>
                <a href="javascript:void(0)">
                    <img src="<%= collectionList[i].src %>">
                    <p class="v-title"><%= collectionList[i].title %></p>
                </a>
            </li>
        <% } %>

    </ul>
</div>
